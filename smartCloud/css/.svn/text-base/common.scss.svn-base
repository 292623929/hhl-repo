/*reset start*/
li{list-style:none}
img{vertical-align:top;border:none}
body{margin: 0}
button{border: 0;padding: 0;cursor: pointer}
a:hover, a:visited, a:link, a:active {
    text-decoration: none;
    color: inherit;
}
/*reset end*/

@mixin trans6 {
    -webkit-transition:.6s;
    -moz-transition:.6s;
    -ms-transition:.6s;
    -o-transition:.6s;
    transition:.6s;
}

@mixin trans3 {
    -webkit-transition: .3s;
    -moz-transition: .3s;
    -ms-transition: .3s;
    -o-transition: .3s;
    transition: .3s;
}

//button
@mixin butt-green{
    cursor: pointer;
    width: 80px;
    height: 34px;
    background-color: rgba(0, 204, 159, 1);
    border-radius: 6px;
    color: $cfff;
    line-height: 34px;
    text-align: center;
    font-size: $f14;
    display: inline-block;
}

@mixin butt-blue{
    cursor: pointer;
    width: 80px;
    height: 34px;
    background-color: rgba(38, 57, 73, 1);
    border-radius: 6px;
    color: $cfff;
    line-height: 34px;
    text-align: center;
    font-size: $f14;
    display: inline-block;
}

@mixin butt-skyblue{
    cursor: pointer;
    width: 80px;
    height: 50px;
    background-color: $c1f;
    color: $cfff;
    line-height: 34px;
    text-align: center;
    font-size: $f14;
    display: inline-block;
}

.animation-fadeIn {
    -webkit-animation: fadeIn 1s;
    -o-animation: fadeIn 1s;
    -ms-animation: fadeIn 1s;
    animation: fadeIn 1s;
}

body{
    min-width: 1200px;
}

a:hover{
    color: #1048FF;
    @include trans6;
}

a{
    @include trans6;
}

.bord-w-button{
    border: 1px solid $cfff;
    height: 33px;
    width: 118px;
    font-size: $f14;
    color: $cfff;
    line-height: 33px;
    text-align: center;
    &:hover{
        color: #262933;
        background-color: #fff;
    }
}

.pull-left{
    float: left;
}

.pull-left{
    float: right;
}

